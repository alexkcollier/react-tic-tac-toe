{
  "ecmaFeatures": {
    "jsx": true
  },
  "env": { "browser": true, "jest": true },
  "extends": ["airbnb", "prettier", "prettier/react"],
  "rules": {
    "linebreak-style": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100
      }
    ]
  },
  "plugins": ["react", "prettier"]
}
